      "blocks": {
        "a5be64aa-305e-4439-b103-1a99f9bed859": {
          "type": "ingredient",
          "settings": {
            "image": "shopify:\/\/shop_images\/Artboard_1ingredients.jpg",
            "title": "Coconut Water",
            "content": "<p>Nature’s source of <strong>potassium <\/strong>(4x bananas and 8-10x sports drinks).<br\/>Contains glucose, antioxidants, magnesium, calcium, and other nutrients.<\/p>"
          }
        },
        "93a5ddec-6ce3-48e5-a7b5-566ad66e6fd3": {
          "type": "ingredient",
          "settings": {
            "image": "shopify:\/\/shop_images\/Artboard_2ingredients.jpg",
            "title": "Pink Himalayan Salt",
            "content": "<p>Nature’s natural source of <strong>sodium.<br\/><\/strong>Sodium is a major electrolyte, so don’t avoid it. It is absolutely imperative for proper hydration.<\/p>"
          }
        },
        "34f9bdc0-a54e-4008-bf45-3ebb71c84d39": {
          "type": "ingredient",
          "settings": {
            "image": "shopify:\/\/shop_images\/Artboard_3ingredients.jpg",
            "title": "No Added Sugar",
            "content": "<p>Added sugars don't include vitamins and minerals found in natural sources of glucose.<br\/>They are used by food manufacturers to increase flavor or extend shelf life.<\/p>"
          }
        },
        "91e069ac-44f6-4fdb-a0e6-c660decec450": {
          "type": "ingredient",
          "settings": {
            "image": "shopify:\/\/shop_images\/Artboard_8ingredients.jpg",
            "title": "Non-GMO Citric Acid",
            "content": "<p>Used for added flavor and naturally found in citrus fruits. <br\/>Ours is from cassava root. <\/p>"
          }
        },
        "9c25f5f1-0ce6-454f-b00a-679f79618a25": {
          "type": "ingredient",
          "settings": {
            "image": "shopify:\/\/shop_images\/Artboard_2_copyingredients.jpg",
            "title": "Lemon Juice Powder",
            "content": "<p>Made from real lemons.<br\/>Added for flavor.<\/p>"
          }
        },
        "41a88c5e-4f24-4e51-98fc-a1cdc3cd46cf": {
          "type": "ingredient",
          "settings": {
            "image": "shopify:\/\/shop_images\/Artboard_6ingredients.jpg",
            "title": "Sodium Citrate",
            "content": "<p>Sodium from citric acid,<br\/>used for function.<\/p>"
          }
        },
        "9af47fd8-f4be-4910-b4b8-4be06804069f": {
          "type": "ingredient",
          "settings": {
            "image": "shopify:\/\/shop_images\/Artboard_5ingredients.jpg",
            "title": "Organic Stevia Extract",
            "content": "<p>Plant-based sweetener with no sugar or calories.<br\/>Despite the common misconception, stevia is not an artificial sweetener.<\/p>"
          }
        },
        "8021f0c9-85a8-45e8-bfa2-5ac66fe0eaa6": {
          "type": "ingredient",
          "settings": {
            "image": "shopify:\/\/shop_images\/Artboard_4ingredients.jpg",
            "title": "Monk Fruit Extract",
            "content": "<p>A vine-ripened fruit native to Asia that helps round out the flavor<br\/>so that there is no bitter aftertaste.<\/p>"
          }
        },
        "52b9bc1c-f6f5-4b29-bbb9-f5409ee17c82": {
          "type": "ingredient",
          "settings": {
            "image": "shopify:\/\/shop_images\/Artboard_7ingredients.jpg",
            "title": "Fruit Juice Powder & Organic Flavors",
            "content": "<p>Added for taste and preference, <br\/>so there is a delicious Cure flavor for everyone.<\/p>"
          }
        }
